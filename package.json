  "dependencies": {
    "@types/supertest": "^2.0.12",
    "axios": "^1.4.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.4",
    "@types/node": "^20.5.4",
    "axios-mock-adapter": "^1.21.5",
    "concurrently": "^8.2.1",
    "jest": "^29.6.4",
